page{
    background: #fff;
}
.logo {
    width: 200rpx;
    margin: 110rpx auto;
}
.logo image{
    width: 200rpx;
    height: 152rpx;
}
.form{
    padding: 0 60rpx;
}
.form .item{
    position: relative;
    padding: 0 0 20rpx;
    margin: 0 0 50rpx;
    border-bottom: 2rpx solid #ccc;
}
.form .item.pass{
    margin: 0 0 30rpx;
}
.form .item image{
    left: 0;
    top: 6rpx;
    width: 40rpx;
    height: 44rpx;
    position: absolute;
}
.form .item input{
    color: #333;
    height: 50rpx;
    font-size: 24rpx;
    padding: 0 0 0 60rpx;
}